{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "format": "prettier --write .",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix",
        "css:fix": "stylelint src/**/*.{css,scss,vue} --fix",
        "check:style": "stylelint **/*.{scss,vue} --custom-syntax postcss-html",
        "style:fix": "stylelint **/*.{scss,vue} --custom-syntax postcss-html --fix",
        "all:fix": "npm run format && npm run style:fix"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "normalize.css": "^8.0.1",
        "pinia": "^2.0.22",
        "postcss": "^8.1.0",
        "swiper": "^8.4.2",
        "vue": "^3.2.37",
        "vue-axios": "^3.4.1",
        "vue-router": "^4.1.5"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "@vitejs/plugin-vue": "^3.1.0",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-vue": "^9.5.1",
        "i": "^0.3.7",
        "npm": "^8.19.2",
        "postcss-html": "^1.5.0",
        "prettier": "^2.7.1",
        "stylelint": "^14.12.0",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard": "^28.0.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-scss": "^4.3.0",
        "stylelint-webpack-plugin": "^3.3.0",
        "typescript": "^4.8.3",
        "vite": "^3.1.0",
        "vite-plugin-svg-icons": "^2.0.1",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^0.40.4",
        "webpack": "^5.74.0"
    }
}
